{
    "name": "mybuilder/when-i-work-api",
    "description": "Library that support connections and retrieve data from WhenIWork.com API",
    "minimum-stability": "stable",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "doctrine/collections": "^1.7",
        "guzzlehttp/guzzle": "^6.2||^7.0",
        "jms/serializer": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": { "MyBuilder\\Library\\WhenIWork\\": "src" }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
