{
    "name": "mybuilder/when-i-work-api",
    "description": "Library that support connections and retrieve data from WhenIWork.com API",
    "minimum-stability": "stable",
    "license": "MIT",
    "version": "0.5.0",
    "require": {
        "php": ">=7.1",
        "jms/serializer": "~1.1",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": { "MyBuilder\\Library\\WhenIWork\\": "src" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
